	<g:HTMLPanel>
		<g:HTMLPanel ui:field="forumTopicsContainer" styleName="forumTopicsContainer"/>
		<g:HTMLPanel ui:field="forumPostsContainer" styleName="forumPostsContainer"/>
		<g:HTMLPanel styleName="forumControls" ui:field="forumControls">
			<c:RaphaelButtonWidget ui:field="addForumPostButton" icon="forum" label="add_forum_post" modifier="add"/>
			<c:RaphaelButtonWidget ui:field="deleteForumPostButton" icon="forum" label="delete_forum_post" modifier="remove"/>
			<c:RaphaelButtonWidget ui:field="editForumPostButton" icon="forum" label="edit_forum_post" modifier="edit"/>
		</g:HTMLPanel>
	</g:HTMLPanel>

</ui:UiBinder> 