<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder' xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:org.bundolo.client.widget" xmlns:ck="urn:import:com.axeiya.gwtckeditor.client">

	<g:HTMLPanel>
		<g:Label ui:field="kindLabel"/><g:ListBox ui:field="kind"></g:ListBox>
		<g:Label ui:field="nameLabel"/><g:TextBox ui:field="name"/>		
		<g:Label ui:field="textLabel"/><ck:CKEditor ui:field="text"/>		
		<g:Label ui:field="childrenLabel"/><c:ExtendedHTMLPanel ui:field="childrenContainer"/>
		<g:HTMLPanel styleName="editPageControls">
			<c:RaphaelButtonWidget ui:field="savePageButton" icon="page" label="save" modifier="confirm"/>
			<c:RaphaelButtonWidget ui:field="cancelPageButton" icon="page" label="cancel" modifier="cancel"/>
		</g:HTMLPanel>
	</g:HTMLPanel>

</ui:UiBinder>