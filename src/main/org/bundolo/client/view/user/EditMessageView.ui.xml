<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder' xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:org.bundolo.client.widget"
xmlns:ck="urn:import:com.axeiya.gwtckeditor.client">

	<g:HTMLPanel>
		<g:Label ui:field="titleLabel"/><g:TextBox ui:field="title"></g:TextBox>
		<g:Label ui:field="textLabel"/><ck:CKEditor ui:field="text"></ck:CKEditor>
		<c:RaphaelButtonWidget ui:field="sendMessageButton" icon="message" label="send_message" modifier="confirm"/>
		<c:RaphaelButtonWidget ui:field="cancelMessageButton" icon="message" label="cancel" modifier="cancel"/>
	</g:HTMLPanel>

</ui:UiBinder>