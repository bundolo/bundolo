<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder' xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:org.bundolo.client.widget"
xmlns:dp="urn:import:com.google.gwt.user.datepicker.client">

	<g:HTMLPanel>
		<g:Label ui:field="displayNameLabel"/><g:TextBox ui:field="username"/><br/>
		<g:Label ui:field="emailAddressLabel"/><g:TextBox ui:field="firstEmail"/><br/>
		<g:Label ui:field="passwordLabel"/><g:PasswordTextBox ui:field="password"/><br/>
		<c:RaphaelButtonWidget ui:field="saveUserProfileButton" icon="user" label="save" modifier="confirm"/>
		<c:RaphaelButtonWidget ui:field="cancelUserProfileButton" icon="user" label="cancel" modifier="cancel"/>
	</g:HTMLPanel>
 
</ui:UiBinder>