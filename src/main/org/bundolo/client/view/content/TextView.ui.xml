<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder' xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:org.bundolo.client.widget">

	<g:HTMLPanel>
		<g:HTMLPanel ui:field="textContainer" styleName="textContainer"/>
		<c:ConditionalPanel ui:field="textControlsPanel">
			<g:HTMLPanel styleName="textControls" ui:field="textControls">
				<c:RaphaelButtonWidget ui:field="addTextButton" icon="texts" label="add_text" modifier="add"/>
				<c:RaphaelButtonWidget ui:field="deleteTextButton" icon="texts" label="delete_text" modifier="remove"/>
				<c:RaphaelButtonWidget ui:field="editTextButton" icon="texts" label="edit_text" modifier="edit"/>
			</g:HTMLPanel>
		</c:ConditionalPanel>
	</g:HTMLPanel>

</ui:UiBinder> 